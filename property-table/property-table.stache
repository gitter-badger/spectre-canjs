<div class="property-table">
  <table class="table table-striped table-condensed">
    <thead>
      <tr>
        <th>Field</th>
        <th>Value</th>
      </tr>
    </thead>
    <tbody>
      {{#each fields}}
        <tr>
          <td>{{alias}}</td>
          <td>{{{getValue(.)}}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
