<div class="form-group">
    <label {{#if properties.inline}} class="hide" {{/if}} for="{{properties.name}}">{{properties.alias}}</label>
    <div class="input-group">
        <select-field {(value)}="day" {properties}="dayProperties" class="float-left" (change)="onChange" />
        <select-field {(value)}="month" {properties}="monthProperties" class="float-left" (change)="onChange" />
        <select-field {(value)}="year" {properties}="yearProperties" class="float-left" (change)="onChange" />
    </div>
    <div class="clear"></div>
</div>
