<div class="form-group">
    <label {{#inline}}class="sr-only" {{/inline}}for="{{name}}">{{alias}}</label>
    <div class="input-group">
    <input type="text" class="form-control" name="{{name}}" id="{{name}}" ($keydown)="beforeSubmit(%element, %event)" ($blur)="onBlur(%element)" placeholder="{{properties.placeholder}}" {($value)}="value">
    <span class="input-group-addon" data-toggle="date-selector">
        <span class="fa fa-calendar"></span>
    </span>
</div>
</div>
